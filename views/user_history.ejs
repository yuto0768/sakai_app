<link rel="stylesheet" href="/public/stylesheet/history.css">
<h1>購入履歴</h1>
<table>
    <tr>
        <th>購入日</th>
        <th>商品名</th>
        <th>金額</th>
    </tr>
    <%for(p of rows){%>
        <tr>
            <td>
                <%=format('yy/MM/dd  hh:mm', p.createdAt)%>
            </td>
            <td>
                <ul>
                    <%for (o of p.options) {%>
                        <li>
                            <%=o.product.name%>
                                <%=o.size%>
                                    <%=o.color%>
                                        <%=o.product.price%>
                        </li>
                        <%}%>
                </ul>
            </td>
        </tr>
        <%}%>
</table>